{
  "manifest_version": 3,
  "name": "TreeVed",
  "version": "1.0.0",
  "description": "Treeved",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "treeved16.ico",
      "48": "treeved48.ico",
      "128": "treeved128.ico"
    }
  },
  "icons": {
    "16": "treeved16.ico",
    "48": "treeved48.ico",
    "128": "treeved128.ico"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service workers": "../src/background.js"
  }
}